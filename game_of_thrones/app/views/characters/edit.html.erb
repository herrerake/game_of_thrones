<h1>Edit the Character</h1>

 <%= form_for @character do |f| %>
   <%= f.label :name %>
   <%= f.text_field :name %>

   <%= f.label :age %>
   <%= f.text_field :age %>

   <%= f.label :img_url %>
   <%= f.text_field :img_url %>

   <p>House <%= select_tag :house_id, options_for_select(@houses) %></p>

   <%= f.submit %>

 <% end %>
