<h1>Edit House</h1>

 <%= form_for @house do |f| %>
   <%= f.label :name %>
   <%= f.text_field :name %>

   <%= f.label :img_url %>
   <%= f.text_field :img_url %>

   <%= f.label :motto %>
   <%= f.text_field :motto %>

   <%= f.label :region %>
   <%= f.text_field :region %>
   <%= f.submit %>

 <% end %>

 <p><%= link_to "Return to The Other Houses", houses_path %></p>
